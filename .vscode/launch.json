	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}"
		},
		{
			"name": "Backend",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/backend/dist/windows-entry",
			"outFiles": ["${workspaceFolder}/backend/dist/**/*.js"],
			"env": {
				"NODE_ENV": "development"
			}
		},
		{
			"name": "Backend (non-shitty-os",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/backend/dist",
			"outFiles": ["${workspaceFolder}/backend/dist/**/*.js"],
			"env": {
				"NODE_ENV": "development"
			}
		}
	]
}
